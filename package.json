{
  "name": "pc-nrfconnect-linkmonitor",
  "version": "0.7.0",
  "description": "LTE Link Monitor",
  "displayName": "LTE Link Monitor",
  "repository": {
    "type": "git",
    "url": "https://github.com/NordicSemiconductor/pc-nrfconnect-linkmonitor.git"
  },
  "author": "Nordic Semiconductor ASA",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "nrfconnect": "^2.4.0"
  },
  "main": "dist/bundle.js",
  "files": [
    "dist/",
    "resources/icon.*",
    "icon.png",
    "LICENSE"
  ],
  "scripts": {
    "dev": "webpack --watch",
    "webpack": "webpack",
    "build": "cross-env NODE_ENV=production webpack",
    "nordic-publish": "nrfconnect-scripts nordic-publish",
    "lint": "eslint components reducers",
    "test": "jest --testResultsProcessor jest-bamboo-formatter",
    "test-watch": "jest --watch",
    "clean": "npm install rimraf && npm run clean-dist && npm run clean-modules",
    "clean-dist": "rimraf dist",
    "clean-modules": "rimraf \"node_modules/!(rimraf|.bin)\""
  },
  "devDependencies": {
    "chart.js": "^2.6.0",
    "debug": "^2.6.8",
    "electron-is-dev": "^0.3.0",
    "leaflet": "^1.2.0",
    "mcc-mnc-list": "^1.0.74",
    "nedb": "^1.8.0",
    "pc-nrfconnect-devdep": "https://github.com/NordicSemiconductor/pc-nrfconnect-devdep.git#v1.3.5",
    "react-chartjs-2": "^2.5.6",
    "react-leaflet": "^1.5.0",
    "react-rangeslider": "^2.2.0",
    "shebang-loader": "0.0.1"
  },
  "dependencies": {
    "electron-store": "^2.0.0",
    "serialport": "^6.2.0"
  },
  "bundledDependencies": [
    "electron-store"
  ],
  "jest": {
    "moduleNameMapper": {
      "nrfconnect/core": "<rootDir>/mocks/coreMock.js",
      "serialport": "<rootDir>/mocks/serialportMock.js",
      "shebang-loader!mcc-mnc-list": "mcc-mnc-list"
    }
  }
}
